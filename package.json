{
  "name": "wiki-map-people",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "test": "npm run build-css && react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "lint": "npm-run-all tslint stylelint",
    "tslint": "tslint -c tslint.json **/*.{ts,tsx}",
    "stylelint": "stylelint --config ./.stylelintrc --syntax scss --color **/*.scss",
    "lint-staged": "lint-staged --config .lintstagedrc",
    "pretty": "prettier --write **/*.{js,ts,tsx,scss}"
  },
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^2.0.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-google-maps": "^9.4.5",
    "react-scripts-ts": "2.17.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/material-ui": "^0.21.5",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/redux": "^3.6.0",
    "husky": "1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "stylelint": "^9.5.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.0",
    "typescript": "^3.0.1"
  }
}
